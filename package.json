{
  "name": "vscode-swissknife",
  "displayName": "Swissknife",
  "description": "The developers swissknife. Do conversions and generations right out of vs code. Extendable with user scripts",
  "version": "1.5.0",
  "author": "luisfontes19",
  "publisher": "luisfontes19",
  "license": "MIT",
  "engines": {
    "vscode": "^1.60.0"
  },
  "repository": {
    "url": "https://github.com/luisfontes19/vscode-swissknife"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "touchBar": [
        {
          "command": "swissknife.show",
          "group": "editing"
        }
      ],
      "explorer/context": [
        {
          "command": "swissknife.toggleCheckedFile",
          "group": "2_workspace",
          "when": "!explorerResourceIsFolder"
        },
        {
          "command": "swissknife.toggleCheckedFolder",
          "group": "2_workspace",
          "when": "explorerResourceIsFolder"
        }
      ],
      "editor/context": [
        {
          "command": "swissknife.copyPathWithLine",
          "when": "editorTextFocus && !editorHasSelection && !editorHasMultipleSelections",
          "group": "9_cutcopypaste"
        }
      ]
    },
    "commands": [
      {
        "command": "swissknife.show",
        "title": "Open Swissknife command pallet",
        "icon": "data/icon.png"
      },
      {
        "command": "swissknife.reload",
        "title": "Reload Swissknife scripts"
      },
      {
        "command": "swissknife.openScripts",
        "title": "Open Swissknife user scripts folder"
      },
      {
        "command": "swissknife.copyPathWithLine",
        "title": "Copy file path with line"
      },
      {
        "command": "swissknife.toggleCheckedFile",
        "title": "Toggle: Check File"
      },
      {
        "command": "swissknife.toggleCheckedFolder",
        "title": "Toggle: Check Folder"
      }
    ],
    "keybindings": [
      {
        "command": "swissknife.show",
        "key": "Ctrl+Shift+9",
        "mac": "Cmd+Shift+9",
        "when": "editorFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run bundle",
    "vscode:package": "vsce package",
    "rename": "node -e \"require('fs').rename('out/index.js', 'out/extension.js', function(err) { if (err) console.log(err); console.log('File successfully renamed!') })\"",
    "bundle": "ncc build src/extension.ts -o out && npm run rename",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "tag": "git tag v$(node -e \"console.log(require('./package.json').version)\")",
    "publish": "npm run bundle && vsce package && vsce publish && npm run tag"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/bip39": "^3.0.0",
    "@types/elliptic": "^6.4.12",
    "@types/express": "^4.17.11",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/request": "^2.48.5",
    "@types/uuid": "^8.3.0",
    "@types/vscode": "^1.60.0",
    "@types/zxcvbn": "^4.4.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.32.0",
    "mocha": "^8.1.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "elliptic": "^6.5.4",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "hash-identifier": "^1.0.1",
    "request": "^2.88.2",
    "selfsigned": "^1.10.8",
    "showdown": "^1.9.1",
    "uuid": "^8.3.1",
    "zxcvbn": "^4.4.2"
  },
  "keywords": [
    "server",
    "serve",
    "sort",
    "swissknife",
    "scriptable",
    "encode",
    "decode",
    "convertion",
    "script",
    "template",
    "insert",
    "developer",
    "sha1",
    "sha256",
    "sha512",
    "bip39",
    "mnemonic",
    "md5",
    "base64",
    "hex",
    "count",
    "words",
    "characters",
    "rsa",
    "public",
    "private",
    "key",
    "keypair",
    "certificate",
    "self signed",
    "rgb",
    "crypto",
    "currency",
    "convertor",
    "url",
    "html",
    "binary",
    "uuid",
    "csv",
    "markdown",
    "random",
    "string",
    "password",
    "timestamp",
    "date",
    "request",
    "fetch",
    "jwt",
    "lorem",
    "ipsum"
  ],
  "icon": "data/swissknife.png"
}